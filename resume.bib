@article{bjornstadInsulinSecretionSensitivity2024,
  title = {Insulin {{Secretion}}, {{Sensitivity}}, and {{Kidney Function}} in {{Young Individuals With Type}} 2 {{Diabetes}}},
  author = {Bjornstad, Petter and Choi, Ye Ji and Platnick, Carson and Gross, Susan and Narongkiatikhun, Phoom and Melena, Isabella and Remmers, Lauryn and Baca, Madison and Schutte, Grant and Dobbs, Tyler and Vigers, Tim and Pyle, Laura and Driscoll, Lynette and Tommerdahl, Kalie and Kendrick, Jessica and Looker, Helen C. and Dart, Allison and Cherney, David and Van Raalte, Daniel H. and Srivastava, Anand and Li, Luping and Prasad, Pottumarthi and Saulnier, Pierre and Nelson, Robert G. and Johnson, Richard J. and Nadeau, Kristen J. and {Bjornstad Lab} and Becker, Nicholas and Best, Kyla and Birznieks, Carissa and Bui, Michelle and Carrasco, Diego and Casillas, Daniel and Flynn, Maureen and Gruszcynski, Audrey and Hall, Lucy and Harbour, Madeline and Leroux, Melissa and Nash, Kelly and Nguyen, Nhung and Sell, Emily and {Rountree-Jablin}, Callyn},
  year = {2024},
  month = mar,
  journal = {Diabetes Care},
  volume = {47},
  number = {3},
  pages = {409--417},
  issn = {0149-5992, 1935-5548},
  doi = {10.2337/dc23-1818},
  abstract = {OBJECTIVE               {$\beta$}-Cell dysfunction and insulin resistance magnify the risk of kidney injury in type 2 diabetes. The relationship between these factors and intraglomerular hemodynamics and kidney oxygen availability in youth with type 2 diabetes remains incompletely explored.                                         RESEARCH DESIGN AND METHODS               Fifty youth with type 2 diabetes (mean age {\textpm} SD 16 {\textpm} 2 years; diabetes duration 2.3 {\textpm} 1.8 years; 60\% female; median HbA1c 6.4\% [25th, 75th percentiles 5.9, 7.6\%]; BMI 36.4 {\textpm} 7.4 kg/m2; urine albumin-to-creatinine ratio [UACR] 10.3 [5.9, 58.0] mg/g) 21 control participants with obesity (OCs; age 16 {\textpm} 2 years; 29\% female; BMI 37.6 {\textpm} 7.4 kg/m2), and 20 control participants in the normal weight category (NWCs; age 17 {\textpm} 3 years; 70\% female; BMI 22.5 {\textpm} 3.6 kg/m2) underwent iohexol and p-aminohippurate clearance to assess glomerular filtration rate (GFR) and renal plasma flow, kidney MRI for oxygenation, hyperglycemic clamp for insulin secretion (acute C-peptide response to glucose [ACPRg]) and disposition index (DI; {\texttimes}103 mg/kg lean/min), and DXA for body composition.                                         RESULTS               Youth with type 2 diabetes exhibited lower DI (0.6 [0.0, 1.6] vs. 3.8 [2.4, 4.5] {\texttimes} 103 mg/kg lean/min; P \&lt; 0.0001) and ACPRg (0.6 [0.3, 1.4] vs. 5.3 [4.3, 6.9] nmol/L; P \&lt; 0.001) and higher UACR (10.3 [5.9, 58.0] vs. 5.3 [3.4, 14.3] mg/g; P = 0.003) and intraglomerular pressure (77.8 {\textpm} 11.5 vs. 64.8 {\textpm} 5.0 mmHg; P \&lt; 0.001) compared with OCs. Youth with type 2 diabetes and OCs had higher GFR and kidney oxygen availability (relative hyperoxia) than NWCs. DI was associated inversely with intraglomerular pressure and kidney hyperoxia.                                         CONCLUSIONS               Youth with type 2 diabetes demonstrated severe {$\beta$}-cell dysfunction that was associated with intraglomerular hypertension and kidney hyperoxia. Similar but attenuated findings were found in OCs.},
  langid = {english},
  file = {/Users/timvigers/Zotero/storage/GMMUUHCA/Bjornstad et al. - 2024 - Insulin Secretion, Sensitivity, and Kidney Function in Young Individuals With Type 2 Diabetes.pdf}
}

